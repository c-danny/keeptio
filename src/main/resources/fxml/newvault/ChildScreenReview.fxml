<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="550.0" prefWidth="750.0" styleClass="kpt-padding-outer" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane styleClass="kpt-gridpane">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Database name" GridPane.rowIndex="1" />
            <Label text="Location" GridPane.rowIndex="2" />
            <Label text="Cipher algorithm" GridPane.rowIndex="3" />
            <Label text="PBKDF rounds" GridPane.rowIndex="5" />
            <Label text="Hash algorithm" GridPane.rowIndex="4" />
            <Label fx:id="dbNameLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="locationLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="cipherLabel" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="hashLabel" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="pbkdfRoundsLabel" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label styleClass="kpt-heading-1" text="Review your settings" />
         </children>
      </GridPane>
   </children>
</VBox>
